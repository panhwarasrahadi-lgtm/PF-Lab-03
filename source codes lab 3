Task 7:
#include <stdio.h>
int main(){
	int a=10;
	float b=11.6;
	double c=11.6666;
	char d='a';
	printf("%d\n",a);
	printf("%f\n",b);
	printf("%lf\n",c);
	printf("%c",d);
}


task 8
#include <stdio.h>
int main(){
	int a;
	float b;
	char c;
	printf("enter a integer\n");
	scanf("%d",&a);
	printf("integer is:%d\n",a);
    printf("enter a decimal number\n");
	scanf("%f",&b);
	printf("number is:%f\n",b);
	printf("enter a charachter\n");
	scanf(" %c",&c);
	printf("charachter is:%c\n",c);
}


Task 9
#include <stdio.h>

int main() {
    double a;
    printf("Enter a decimal number: ");
    scanf("%lf", &a);
    printf("Default precision: %lf\n", a);
    printf("Two decimal places: %.2lf\n", a);
    printf("Five decimal places: %.5lf\n", a);
    return 0;
}
